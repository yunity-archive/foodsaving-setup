{
  "apps": [
    {
      "name": "proxy",
      "script": "proxy/index.js"
    },
    {
      "name": "mobile",
      "script": "npm",
      "args": [
        "run", "dev-server"
      ],
      "cwd": "yunity-webapp-mobile"
    },
    {
      "name": "sockets",
      "script": "index.js",
      "args": [
        "-w", "8080",
        "-p", "9080"
      ],
      "cwd": "yunity-sockets"
    },
    {
      "name": "django",
      "script": "manage.py",
      "args": [
        "runserver",
        "--noreload",
        "8000"
      ],
      "exec_interpreter": "env/bin/python",
      "exec_mode": "fork_mode",
      "cwd": "yunity-core"
    }
  ]
}
