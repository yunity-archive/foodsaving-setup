{
  "apps": [
    {
      "name": "sockets",
      "script": "index.js",
      "args": [
        "-w", "8080",
        "-p", "9080"
      ],
      "cwd": "foodsaving-sockets"
    },
    {
      "name": "django",
      "script": "manage.py",
